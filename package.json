{
  "name": "react-native-code-scanner",
  "version": "1.0.6",
  "description": "react-native-code-scanner is a powerful and flexible library for scanning barcodes and QR codes in React Native applications.",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "files": [
    "src",
    "lib",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "prepare": "bob build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zvukamar/react-native-code-scanner.git"
  },
  "keywords": [
    "react-native",
    "code-scanner",
    "barcode-scanner",
    "scanner",
    "qr-scanner",
    "native-modules",
    "ios",
    "android",
    "vision-camera"
  ],
  "author": "Alex",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zvukamar/react-native-code-scanner/issues"
  },
  "homepage": "https://github.com/Zvukamar/react-native-code-scanner#readme",
  "devDependencies": {
    "@types/react": "^18.3.2",
    "react-native-builder-bob": "^0.23.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "@mgcrea/vision-camera-barcode-scanner": "^0.11.2",
    "react": "*",
    "react-native": "*",
    "react-native-vision-camera": "^4.0.4",
    "react-native-worklets-core": "^1.3.3"
  },
  "react-native": "src/index.ts",
  "source": "src/index.ts",
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ]
}